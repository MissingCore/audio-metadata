{
  "name": "@missingcore/audio-metadata",
  "version": "1.1.1",
  "description": "An audio file metadata reader built for React Native.",
  "main": "lib/module/index",
  "module": "lib/module/index",
  "types": "lib/typescript/index.d.ts",
  "react-native": "src/index",
  "source": "src/index",
  "files": [
    "src",
    "lib",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "android",
    "ios",
    "cpp",
    "*.podspec",
    "!ios/build",
    "!android/build",
    "!android/gradle",
    "!android/gradlew",
    "!android/gradlew.bat",
    "!android/local.properties",
    "!**/.*"
  ],
  "scripts": {
    "example": "pnpm --filter @missingcore/audio-metadata-example run",
    "bootstrap": "pnpm install && pnpm example android",
    "typecheck": "tsc --noEmit",
    "test": "jest --coverage",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "clean": "del-cli lib",
    "prepare": "bob build",
    "release": "release-it"
  },
  "keywords": [
    "react-native",
    "expo",
    "ios",
    "android",
    "audio",
    "metadata"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MissingCore/audio-metadata.git"
  },
  "author": "cyanChill",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MissingCore/audio-metadata/issues"
  },
  "homepage": "https://github.com/MissingCore/audio-metadata#readme",
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "fast-text-encoding": "^1.0.6",
    "fastestsmallesttextencoderdecoder": "^1.0.22"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^19.2.2",
    "@dr.pogodin/react-native-fs": "^2.27.1",
    "@evilmartians/lefthook": "^1.6.18",
    "@react-native/eslint-config": "^0.74.84",
    "@release-it/keep-a-changelog": "^5.0.0",
    "@types/fast-text-encoding": "^1.0.3",
    "@types/fastestsmallesttextencoderdecoder": "^1.0.2",
    "@types/jest": "^29.5.12",
    "@types/react": "~18.2.79",
    "commitlint": "^19.3.0",
    "del-cli": "^5.1.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "expo-file-system": "^17.0.1",
    "jest": "^29.7.0",
    "prettier": "^3.3.2",
    "react": "18.2.0",
    "react-native": "0.74.2",
    "react-native-builder-bob": "^0.23.2",
    "release-it": "^17.4.0",
    "release-it-pnpm": "^4.6.2",
    "typescript": "^5.5.2"
  },
  "peerDependencies": {
    "@dr.pogodin/react-native-fs": ">=2.22.0",
    "@types/react": ">=18.2.0",
    "expo-file-system": ">=17.0.0",
    "react": ">=18.2.0",
    "react-native": ">=0.74.0"
  },
  "peerDependenciesMeta": {
    "@dr.pogodin/react-native-fs": {
      "optional": true
    },
    "@types/react": {
      "optional": true
    },
    "expo-file-system": {
      "optional": true
    },
    "react": {
      "optional": true
    },
    "react-native": {
      "optional": true
    }
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  }
}
